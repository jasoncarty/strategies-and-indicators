  "services": {
    "analytics": {
      "host": "0.0.0.0",
      "port": 5001,
      "debug": true,
      "workers": 1
    },
    "ml_service": {
      "host": "0.0.0.0",
      "port": 5002,
      "debug": true,
      "workers": 1
    }

# Docker Build Environment
# Copy this file to build.env and customize for your environment

# Service Ports (used as build arguments)
ANALYTICS_PORT=5001
ML_SERVICE_PORT=5002

# Service Hosts (used as build arguments)
ANALYTICS_HOST=0.0.0.0
ML_SERVICE_HOST=0.0.0.0

# Python Version
PYTHON_VERSION=3.11

# Build Settings
DOCKER_BUILDKIT=1
COMPOSE_DOCKER_CLI_BUILD=1

# Image Tags
ANALYTICS_IMAGE_TAG=latest
ML_SERVICE_IMAGE_TAG=latest
NGINX_IMAGE_TAG=latest

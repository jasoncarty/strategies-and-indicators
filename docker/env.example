ML_SERVICE_IMAGE_TAG=latest
NGINX_IMAGE_TAG=latest

# Build arguments
PYTHON_VERSION=3.11
NODE_VERSION=18

# Development overrides
ANALYTICS_DEV_MODE=true
ML_SERVICE_DEV_MODE=true
